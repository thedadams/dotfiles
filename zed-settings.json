{
  "buffer_font_weight": 400.0,
  "colorize_brackets": true,
  "base_keymap": "JetBrains",
  "title_bar": {
    "show_branch_icon": true
  },
  "icon_theme": "Bearded Icon Theme",
  "file_types": {
    "Dockerfile": ["Dockerfile*"],
    "Git Commit": ["**/*.jjdescription"],
    "Git Attributes": ["{git,.git,.git/info}/attributes"],
    "Git Config": ["{git,.git/modules,.git/modules/*}/config"],
    "Git Ignore": ["{git,.git}/ignore", ".git/info/exclude"]
  },
  "lsp": {
    "wakatime": {
      "initialization_options": {
        "api-key": "<API_KEY>"
      }
    }
  },
  "agent": {
    "profiles": {
      "local": {
        "name": "Local",
        "tools": {
          "fetch": true,
          "find_path": true,
          "grep": true,
          "list_directory": true,
          "move_path": true,
          "read_file": true,
          "terminal": true,
          "thinking": true,
          "web_search": true
        },
        "enable_all_context_servers": false,
        "context_servers": {}
      }
    },
    "default_profile": "write",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4-5"
    },
    "use_modifier_to_send": false
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "terminal": {
    "shell": {
      "with_arguments": {
        "program": "/opt/homebrew/bin/nu",
        "args": ["--config", "~/.config.nu", "--env-config", "~/.env.nu"]
      }
    },
    "dock": "right",
    "cursor_shape": "bar"
  },
  "languages": {
    "Git Commit": {
      "wrap_guides": [72, 80],
      "preferred_line_length": 80
    }
  },
  "theme": "Ayu Dark",
  "theme_overrides": {
    "Ayu Dark": {
      "syntax": {
        "constant.comment.todo": {
          "color": "#27618cff"
        },
        "string.comment.info": {
          "color": "#567627ff"
        },
        "keyword.comment.warn": {
          "color": "#92582bff"
        },
        "property.comment.error": {
          "color": "#83353bff"
        }
      }
    }
  },
  "vim_mode": true,
  "vim": {
    "use_system_clipboard": "never"
  },
  "project_panel": {
    "hide_root": false,
    "indent_size": 13.0,
    "entry_spacing": "standard",
    "auto_fold_dirs": false
  },
  "ui_font_size": 14.0,
  "buffer_font_size": 13,
  "ui_font_family": "Berkeley Mono",
  "buffer_font_family": "Berkeley Mono",
  "confirm_quit": true
}
